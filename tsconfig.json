{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ESNext","dom"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "experimentalDecorators": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@figma"
    ],
    "paths": {
      "jszip": [
        "./node_modules/jszip.js"
      ]
    },
    "outDir": "./dist",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
  },
  "include": ["src/**/*.ts" , "src/assets/*.png"],
}
